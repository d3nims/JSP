DROP DATABASE IF EXISTS test1;

CREATE DATABASE test1;

USE test1;

CREATE TABLE `member` (
id INT(5) PRIMARY KEY AUTO_INCREMENT,
loginId CHAR(200),
loginPw CHAR(200),
nickname CHAR(200),
regDate DATETIME NOT NULL
);

INSERT INTO `member`
SET loginId = 'user1',
loginPw = 'pass1',
nickname = 'name1',
regDate = NOW();

